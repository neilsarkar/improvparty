  "description": "Improv Party",
  "main": "index.js",
  "dependencies": {
    "http-server": "~0.7.2"
  },
  "devDependencies": {
    "http-server": "^0.7.2",
    "protractor": "^1.3.1",
    "webdriver-manager": "^1.0.0",
    "karma": "^0.12.24"
  },
  "scripts": {
